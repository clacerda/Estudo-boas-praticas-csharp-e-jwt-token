@model Login


<form>
  <div class="form-group">
    <label for="userText">Usuário::</label>
    <input type="text" class="form-control" id="userText" aria-describedby="Usuário" placeholder="Digite seu nome de usuário">
  </div>
  <div class="form-group">
    <label for="password">Password</label>
    <input type="password" class="form-control" id="password" placeholder="Digite seu password">
  </div> 
    
  <button type="submit" class="btn btn-primary">Acessar</button>
</form>